<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8" >
  <title>{{ title }}</title>
  <meta name="description" content="{{ description }}">
  <meta name="generator" content="metalsmith">
</head>
<body>
  <div class="container">

    <header>
      <nav>
        {{ sitename }} |
        <a href="/">Home</a> |
        <a href="/about">About</a>
      </nav>
      <h1>{{ title }}<time class="float-right">{{ formattedDate date }}</time></h1>
      <img src="{{ thumbnail }}" alt="Post main image">
    </header>

    <main>
      {{{ contents }}}
    </main>

    <footer>
      <p>Generated with {{ generator }} &mdash; <a href="{{ url }}">{{ url }}</a></p>
    </footer>

  </div>

</body>
</html>
